{
    "name": "aplus/sample",
    "type": "project",
    "description": "Aplus Framework Sample Project",
    "keywords": [
        "sample",
        "template"
    ],
    "homepage": "https://docs.aplus-framework.com/guides/projects/sample/",
    "license": "MIT",
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^1.9",
        "ergebnis/composer-normalize": "^2.15",
        "phpmd/phpmd": "^2.10",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Framework\\Sample\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Sample\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://gitlab.com/aplus-framework/projects/sample/issues",
        "wiki": "https://gitlab.com/aplus-framework/projects/sample/wikis/home",
        "source": "https://gitlab.com/aplus-framework/projects/sample",
        "docs": "https://docs.aplus-framework.com/guides/projects/sample/"
    }
}
