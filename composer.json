{
    "name": "aplus/template",
    "description": "Aplus Framework Template Project",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "sample",
        "template"
    ],
    "authors": [
        {
            "name": "Natan Felles",
            "email": "natanfelles@gmail.com",
            "homepage": "https://natanfelles.github.io"
        }
    ],
    "homepage": "https://aplus-framework.com/packages/template",
    "support": {
        "email": "support@aplus-framework.com",
        "issues": "https://github.com/aplus-framework/template/issues",
        "forum": "https://aplus-framework.com/forum",
        "source": "https://github.com/aplus-framework/template",
        "docs": "https://docs.aplus-framework.com/guides/projects/template/"
    },
    "funding": [
        {
            "type": "Aplus Sponsor",
            "url": "https://aplus-framework.com/sponsor"
        }
    ],
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "aplus/coding-standard": "^2.8",
        "ergebnis/composer-normalize": "^2.44",
        "phpmd/phpmd": "^2.15",
        "phpstan/phpstan": "^1.12",
        "phpunit/phpunit": "^9.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Framework\\Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Template\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        },
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
