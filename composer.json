{
	"name": "natanfelles/sample-package",
	"type": "project",
	"description": "The Framework Sample package",
	"keywords": [
		"sample-package"
	],
	"homepage": "https://the-framework.gitlab.io/sample-package.html",
	"license": "proprietary",
	"authors": [
		{
			"name": "Natan Felles",
			"email": "natanfelles@gmail.com"
		}
	],
	"require": {
		"php": ">=7.3"
	},
	"require-dev": {
		"ext-xdebug": "*",
		"localheinz/composer-normalize": "^1.3",
		"natanfelles/coding-standard": "^1.0",
		"phpmd/phpmd": "^2.7",
		"phpstan/phpstan": "^0.11",
		"phpunit/phpunit": "^8.3"
	},
	"autoload": {
		"psr-4": {
			"Framework\\Sample\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Sample\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://the-framework.gitlab.io/satis/"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"scripts": {
		"post-update-cmd": [
			"composer dump-autoload"
		]
	},
	"support": {
		"email": "natanfelles@gmail.com",
		"issues": "https://gitlab.com/the-framework/sample-package/issues",
		"wiki": "https://gitlab.com/the-framework/sample-package/wikis/home",
		"source": "https://gitlab.com/the-framework/sample-package",
		"docs": "https://the-framework.gitlab.io/sample-package/docs/"
	}
}
